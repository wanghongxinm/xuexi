<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".menu.notification.NotificationFragment">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="100dp"
            android:background="@drawable/bg_half_rounded2"
            android:orientation="vertical">

            <RelativeLayout
                android:id="@+id/N_R_4"
                android:layout_width="120dp"
                android:layout_height="50dp"
                android:layout_below="@id/image_1"
                android:layout_marginStart="230dp"
                android:layout_marginTop="90dp"
                android:layout_marginEnd="30dp"
                android:background="@drawable/bg_header">

                <TextView
                    android:layout_width="80dp"
                    android:layout_height="50dp"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="15dp"
                    android:layout_marginEnd="20dp"
                    android:layout_marginBottom="10dp"
                    android:background="@color/white"
                    android:fontFamily="@font/nunito_bold"
                    android:text="@string/chat_robot"
                    android:textAllCaps="false"
                    android:textSize="18sp" />
            </RelativeLayout>


        </LinearLayout>

        <LinearLayout
            android:id="@+id/f_L_1"
            android:visibility="visible"
            android:layout_width="match_parent"
            android:layout_height="150dp"
            android:layout_marginStart="25dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="25dp"
            android:background="@drawable/bg_header"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="20dp"
                    android:fontFamily="@font/nunito_black"
                    android:text="@string/My_Account"
                    android:textColor="@color/dark_late_gray"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/message_user"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:fontFamily="@font/nunito_black"
                    android:text="15020345"
                    android:textColor="@color/light_sea_green"
                    android:textSize="18sp" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="3dp"
                android:layout_marginStart="15dp"
                android:layout_marginEnd="15dp"
                android:background="@drawable/bg_search" />

            <LinearLayout
                android:id="@+id/container_choose_service"
                android:layout_width="370dp"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                tools:ignore="RtlSymmetry">

                <RelativeLayout
                    android:id="@+id/N_R_1"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content">

                    <ImageView
                        android:id="@+id/image_1"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_centerHorizontal="true"
                        android:background="@drawable/bg_item_choose_services"
                        android:padding="12dp"
                        android:src="@drawable/message"
                        tools:ignore="ContentDescription" />

                    <TextView
                        android:id="@+id/message_1"
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/image_1"
                        android:layout_marginStart="5dp"
                        android:layout_marginTop="5dp"
                        android:fontFamily="@font/nunito_bold"
                        android:text="@string/message_1"
                        android:textAlignment="center"
                        android:textColor="@color/dark_late_gray"
                        android:textSize="15sp"
                        tools:ignore="SmallSp" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/N_R_2"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content">

                    <ImageView
                            android:id="@+id/image_2"
                            android:layout_width="50dp"
                            android:layout_height="50dp"
                            android:layout_centerHorizontal="true"
                            android:background="@drawable/bg_item_choose_services"
                            android:padding="12dp"
                            android:src="@drawable/message1"
                            tools:ignore="ContentDescription"/>

                    <TextView
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/image_2"
                        android:layout_marginTop="5dp"
                        android:fontFamily="@font/nunito_bold"
                        android:text="@string/message_2"
                        android:textAlignment="center"
                        android:textColor="@color/dark_late_gray"
                        android:textSize="15sp"
                        tools:ignore="SmallSp" />
                </RelativeLayout>


                <RelativeLayout
                    android:id="@+id/N_R_3"
                    android:layout_marginEnd="10dp"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content">

                    <ImageView
                        android:id="@+id/image_4"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_centerHorizontal="true"
                        android:background="@drawable/bg_item_choose_services"
                        android:padding="12dp"
                        android:src="@drawable/message2"
                        tools:ignore="ContentDescription" />

                    <TextView
                            android:layout_width="90dp"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/image_4"
                            android:layout_marginTop="5dp"
                            android:gravity="center"
                            android:fontFamily="@font/nunito_bold"
                            android:text="@string/message_4"
                            android:textAlignment="center"
                            android:textColor="@color/dark_late_gray"
                            android:textSize="15sp"
                            tools:ignore="SmallSp"/>
                </RelativeLayout>

            </LinearLayout>

        </LinearLayout>


        <!--界面布局2-->

        <LinearLayout
            android:id="@+id/f_L_11"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="150dp"
            android:layout_marginStart="25dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="25dp"
            android:background="@drawable/bg_header"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="20dp"
                    android:fontFamily="@font/nunito_black"
                    android:text="@string/My_Account"
                    android:textColor="@color/dark_late_gray"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/message_user1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:fontFamily="@font/nunito_black"
                    android:text="15020345"
                    android:textColor="@color/light_sea_green"
                    android:textSize="18sp" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="3dp"
                android:layout_marginStart="15dp"
                android:layout_marginEnd="15dp"
                android:background="@drawable/bg_search" />

            <LinearLayout
                android:id="@+id/container_choose_service1"
                android:layout_width="370dp"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                tools:ignore="RtlSymmetry">

                <RelativeLayout
                    android:id="@+id/N_R_11"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content">

                    <ImageView
                        android:id="@+id/image_11"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_centerHorizontal="true"
                        android:background="@drawable/bg_item_choose_services"
                        android:padding="12dp"
                        android:src="@drawable/message"
                        tools:ignore="ContentDescription" />

                    <TextView
                        android:id="@+id/message_11"
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/image_11"
                        android:layout_marginStart="5dp"
                        android:layout_marginTop="5dp"
                        android:fontFamily="@font/nunito_bold"
                        android:text="@string/message_1"
                        android:textAlignment="center"
                        android:textColor="@color/dark_late_gray"
                        android:textSize="15sp"
                        tools:ignore="SmallSp" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/N_R_22"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content">

                    <ImageView
                        android:id="@+id/image_22"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_centerHorizontal="true"
                        android:background="@drawable/bg_item_choose_services"
                        android:padding="12dp"
                        android:src="@drawable/message1"
                        tools:ignore="ContentDescription"/>

                    <TextView
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/image_22"
                        android:layout_marginTop="5dp"
                        android:fontFamily="@font/nunito_bold"
                        android:text="@string/message_2"
                        android:textAlignment="center"
                        android:textColor="@color/dark_late_gray"
                        android:textSize="15sp"
                        tools:ignore="SmallSp" />
                </RelativeLayout>


                <RelativeLayout
                    android:id="@+id/N_R_33"
                    android:layout_marginEnd="10dp"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content">

                    <ImageView
                        android:id="@+id/image_44"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_centerHorizontal="true"
                        android:background="@drawable/bg_item_choose_services"
                        android:padding="12dp"
                        android:src="@drawable/message2"
                        tools:ignore="ContentDescription" />

                    <TextView
                        android:layout_width="90dp"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/image_44"
                        android:layout_marginTop="5dp"
                        android:gravity="center"
                        android:fontFamily="@font/nunito_bold"
                        android:text="@string/message_4"
                        android:textAlignment="center"
                        android:textColor="@color/dark_late_gray"
                        android:textSize="15sp"
                        tools:ignore="SmallSp"/>
                </RelativeLayout>

            </LinearLayout>

        </LinearLayout>


    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>